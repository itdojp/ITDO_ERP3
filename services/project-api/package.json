{
  "name": "project-api",
  "version": "0.1.0",
  "private": true,
  "description": "Project management and chat integration API (NestJS)",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prestart": "npm run prisma:generate",
    "prestart:dev": "npm run prisma:generate",
    "build": "nest build",
    "prebuild": "npm run prisma:generate",
    "lint": "eslint --ext .ts src",
    "test": "jest",
    "pretest": "npm run prisma:generate",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prisma:generate": "npm run prisma:generate:default && npm run prisma:generate:ckm",
    "prisma:generate:default": "prisma generate",
    "prisma:generate:ckm": "prisma generate --schema prisma/ckm.prisma",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:ckm:diff": "prisma migrate diff --from-empty --to-schema-datamodel prisma/ckm.prisma --script",
    "prisma:db:ckm:execute:init": "prisma db execute --schema prisma/ckm.prisma --file prisma/ckm-migrations/20251017000000_init/migration.sql",
    "prisma:seed:ckm": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/ckm-seed.ts",
    "prisma:seed": "prisma db seed",
    "db:setup": "npm run prisma:migrate && npm run prisma:seed"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.908.0",
    "@aws-sdk/client-s3": "^3.664.0",
    "@aws-sdk/client-secrets-manager": "^3.664.0",
    "@aws-sdk/client-ses": "^3.664.0",
    "@nestjs/apollo": "^12.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/websockets": "^10.4.20",
    "@prisma/client": "^5.11.0",
    "apollo-server-express": "^3.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.25.0",
    "hot-shots": "^10.0.1",
    "mjml": "^4.15.3",
    "pdfkit": "^0.15.0",
    "pg": "^8.12.0",
    "pgvector": "^0.1.6",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.12.7",
    "@types/pdfkit": "^0.13.8",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
